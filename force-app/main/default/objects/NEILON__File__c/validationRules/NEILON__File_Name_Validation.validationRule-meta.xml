<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NEILON__File_Name_Validation</fullName>
    <active>true</active>
    <description>The file name cannot contain special characters  &quot;/&quot; and &quot;?&quot;.</description>
    <errorConditionFormula>OR(CONTAINS( Name , &quot;/&quot;), CONTAINS( Name , &quot;?&quot;))</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>The file name cannot contain special characters  &quot;/&quot; and &quot;?&quot;.</errorMessage>
</ValidationRule>
